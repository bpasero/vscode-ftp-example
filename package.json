{
    "name": "vscode-ftp",
    "displayName": "vscode-ftp",
    "description": "",
    "version": "0.0.1",
    "publisher": "benjpas",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
       "onView:vscode.ftp"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "openFtpResource",
            "title": "Open FTP Resource"
        }],
        "views": {
            "explorer": [
                {
                    "id": "vscode.ftp",
                    "name": "FTP Explorer"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "ftp": "^0.3.10"
    }
}
